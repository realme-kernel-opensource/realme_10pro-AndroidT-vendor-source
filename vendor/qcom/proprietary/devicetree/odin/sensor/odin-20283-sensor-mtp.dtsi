#include <dt-bindings/clock/qcom,gcc-holi.h>

&soc {
	//#ifdef OPLUS_FEATURE_SENSOR_SMEM
        /*enum sensor_id { OPPO_ACCEL,OPPO_GYRO,OPPO_MAG,OPPO_LIGHT,OPPO_PROXIMITY,OPPO_SAR,OPPO_CT,OPPO_BAROMETER,SENSORS_NUM};*/
        sensor_fb: sensor_fb {
            compatible = "oplus,sensor-feedback";
            master-id  = <3>;
            smem-id    = <608>;
        };
	oplus_sensor {
			compatible = "oplus,sensor-devinfo";
			als-row-coe = <110>;
			/*enum {LSM6DSM = 0x01, BMI160 = 0x02, LSM6DS3 = 0x04, BMI260 = 0x08};*/
			gsensor@0 {
					sensor-name = <0x04>;//LSM6DS3
					sensor-direction = <0>;
					sensor-index = <0>;//source num
					sensor-type = <0>;//OPPO_ACCEL
			};
			gsensor@1 {
					sensor-name = <0x02>;//BMI160
					sensor-direction = <4>;
					sensor-index = <1>;//source num
					sensor-type = <0>;//OPPO_ACCEL
			};
			/* enum {AKM0991X = 0x01, MMC5603 = 0x02, MXG4300 = 0x04};*/
			msensor@0 {
					sensor-name = <0x02>;//MMC5603
					sensor-direction = <0>;
					sensor-index = <0>;
					sensor-type = <2>;//OPPO_MAG
					parameter-number= <18>;
					soft-mag-parameter = <9980 0 340 0 510 0 374 1 9456 0 346 0 258 1 54 0 10344 0>;//e00 sign e01 sign...
			};
			msensor@1 {
					sensor-name = <0x01>;//AKM0991X
					sensor-direction = <0>;
					sensor-index = <1>;
					sensor-type = <2>;//OPPO_MAG
					parameter-number= <18>;
					soft-mag-parameter = <10302 0 157 1 67 1 20 0 9705 0 82 1 241 0 195 1 10361 0>;//e00 sign e01 sign...
			};
			/*enum {STK3A5X=0x01,TCS3701=0x02,TCS3408=0x04,STK326X=0x08,MN78911=0x10};*/
			/*enum {NORMAL=0x01,UNDER_LCD=0x02,VIRTUAL_UNDER_LCD=0x03}*/
			lsensor@0 {
					sensor-name = <0x10>;//MN78911
					sensor-index = <0>;
					sensor-type = <3>;//OPPO_ALS
					bus-number = <2>;
					irq-number = <98>;
					als-type = <1>;//NORMAL
					is-unit-device = <1>;
					is-als-dri = <1>;
					als-factor = <250>;
					is_als_initialed = <0>;
					als_buffer_length = <10>;
			};
			lsensor@1 {
					sensor-name = <0x01>;//STK3A5X
					sensor-index = <1>;
					sensor-type = <3>;//OPPO_ALS
					bus-number = <2>;
					irq-number = <98>;
					als-type = <1>;//NORMAL
					is-unit-device = <1>;
					is-als-dri = <1>;
					als-factor = <250>;
					is_als_initialed = <0>;
					als_buffer_length = <10>;
			};
			psensor@0 {
					sensor-name = <0x10>;//MN78911
					sensor-index = <0>;
					sensor-type = <4>;//OPPO_PROXIMITY
					bus-number = <2>;
					irq-number = <98>;
					ps-cail-type = <1>;
					ps-type = <1>;
					is_ps_initialzed = <0>;
					low_step = <300>;
					high_step = <400>;
					low_limit = <650>;
					high_limit = <1050>;
					dirty_low_step = <300>;
					dirty_high_step = <400>;
					ps_dirty_limit = <1800>;
					ps_ir_limit = <2000>;
					ps_adjust_min = <0>;
					ps_adjust_max = <1850>;
					sampling_count = <5>;
					step_max = <400>;
					step_min = <100>;
					step_div = <1500>;
					anti_shake_delta = <70>;
					dynamic_cali_max = <5000>;
					raw2offset_radio = <1000>;
					offset_max = <60000>;
					offset_range_min = <0>;// -255
					offset_range_max = <65535>;
					force_cali_limit = <2000>;
					cali_jitter_limit = <20>;
					cal_offset_margin = <3000>;
			};
			psensor@1 {
					sensor-name = <0x01>;//STK3A5X
					sensor-index = <1>;
					sensor-type = <4>;//OPPO_PROXIMITY
					bus-number = <2>;
					irq-number = <98>;
					ps-cail-type = <1>;
					ps-type = <1>;
					is_ps_initialzed = <0>;
					low_step = <300>;
					high_step = <400>;
					low_limit = <650>;
					high_limit = <1050>;
					dirty_low_step = <300>;
					dirty_high_step = <400>;
					ps_dirty_limit = <1800>;
					ps_ir_limit = <2000>;
					ps_adjust_min = <0>;
					ps_adjust_max = <1850>;
					sampling_count = <5>;
					step_max = <400>;
					step_min = <100>;
					step_div = <1500>;
					anti_shake_delta = <70>;
					dynamic_cali_max = <5000>;
					raw2offset_radio = <1000>;
					offset_max = <60000>;
					offset_range_min = <0>;// -255
					offset_range_max = <65535>;
					force_cali_limit = <2000>;
					cali_jitter_limit = <20>;
					cal_offset_margin = <3000>;
			};
			/*virtual sensor
			enum sensor_algo_id {
					OPPO_PICKUP_DETECT,
					OPPO_LUX_AOD,
					OPPO_TP_GESTURE,
					OPPO_FP_DISPLAY,
					OPPO_FREE_FALL,
					OPPO_CAMERA_PROTECT,
					SENSOR_ALGO_NUM
			}*/
			pickup_detect {
					sensor-type = <0>; //OPPO_PICKUP_DETECT
					is-virtual-sensor;
					is-need-prox = <1>;
					prox-type = <0>; // 0 - proximity; 1 - proximity_fake
			};
	};
	//#endif OPLUS_FEATURE_SENSOR_SMEM
};

