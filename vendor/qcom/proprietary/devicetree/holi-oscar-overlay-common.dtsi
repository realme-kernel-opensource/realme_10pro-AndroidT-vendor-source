&qupv3_se8_i2c {
	#address-cells = <1>;
	#size-cells = <0>;

	status = "disabled";

	goodix19537@5d {
		compatible = "Goodix-gt9886";
		reg = <0x5d>;
		chip-name = "GT9886";

		/* Interrupt && Irq-gpio */
		interrupt-parent = <&tlmm>;
		interrupts = <22 0x2002>;

